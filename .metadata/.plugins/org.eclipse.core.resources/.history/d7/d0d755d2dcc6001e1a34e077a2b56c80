package homework.sessions;

import java.util.Random;

public class GenRandom {
	
	int rand(int n) {
		int num=0;
		for (int i=0;i<500;i++) {
			Random r=new Random();
			num = r.nextInt(901)+100;
			System.out.print(" "+Math.random());
			
		}
		return num;
	}

	public static void main(String[] args) {
		
		GenRandom gr=new GenRandom();
		gr.rand(5);
		
	}

}
