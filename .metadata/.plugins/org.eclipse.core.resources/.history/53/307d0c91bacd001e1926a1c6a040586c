package pracice.files;

import java.io.File;
import java.io.IOException;

public class DemoFiles {

	public static void main(String[] args) {
		String filepath1="C:\\files\\test1.txt";
		File f1=new File(filepath1);
		
		if(!f1.exists()) {
			
			try {
				f1.createNewFile();
			} catch (IOException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			
		}
		
		
		
	}

}
