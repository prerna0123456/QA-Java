package kg.programs;

import java.util.Arrays;

public class IfArrSorted {

	public static void main(String[] args) {
		int[] a= {10,45,15,74,65};
		int[] b;
		for(int i=0;i<a.length;i++) {
			b[i]=a[i];
		}
		Arrays.sort(a);
		if(a.equals(b))
			System.out.println("Array is sorted");
		else
			System.out.println("Array is not sorted");
		
		
	}

}
